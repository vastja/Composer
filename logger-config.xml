<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="obrazovka" target="SYSTEM_OUT">
      <PatternLayout pattern="%c{1}.%m() %level - %msg%n"/>
    </Console>
  </Appenders>   
  <Loggers>
    <Root level="DEBUG">
      <AppenderRef ref="obrazovka"/>
    </Root>
  </Loggers>
</Configuration>